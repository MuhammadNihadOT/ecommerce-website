<section>
  <div class="container">
    <table class="table mt-4">
      <thead>
        <tr>
          <th scope="col">Date</th>
          <th scope="col">Name</th>
          <th scope="col">Email Address</th>
        </tr>
      </thead>
      <tbody>
        {{#each userData}}
        <tr>
          <td>{{this.date}}</td>
          <td>{{this.name}}</td>
          <td>{{this.emailaddr}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</section>